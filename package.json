{
    "name": "incident_management_system",
    "version": "1.0.0",
    "author": "Siddhartha Bokaria",
    "description": "Simple example of incident management system, implementing RESTful API with Node.js and passport",
    "private": true,
    "scripts": {
        "start": "node ./bin/www"
    },
    "jshintConfig": {
        "curly": true,
        "eqeqeq": true,
        "immed": true,
        "latedef": false,
        "newcap": false,
        "noarg": true,
        "sub": true,
        "undef": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
