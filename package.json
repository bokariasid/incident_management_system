        "moment": "^2.22.2",
        "mongoose": "^5.3.2",
        "nconf": "^0.10.0",
        "node-cron": "^2.0.3",
        "nodemon": "^1.18.4",
        "oauth2orize": "^1.11.0",
        "passport": "^0.4.0",
        "passport-http": "^0.3.0",
        "passport-http-bearer": "^1.0.1",
        "passport-oauth2-client-password": "^0.1.2"
